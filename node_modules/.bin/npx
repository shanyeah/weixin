#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_npm@6.1.0@npm/bin/npx-cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../_npm@6.1.0@npm/bin/npx-cli.js" "$@"
  ret=$?
fi
exit $ret
